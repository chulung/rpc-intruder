<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.0.xsd">
    <context:component-scan base-package="com.wchukai"/>
    <bean class="com.wchukai.rpcintruder.client.RpcServiceBeanInit">
        <property name="classNames">
            <list>
                <value>com.wchukai.service.HelloWorldServie</value>
            </list>
        </property>
        <property name="checkEnv" value="false"/>
        <property name="url" value="http://127.0.0.1/rpc-intruder"/>
    </bean>
</beans>