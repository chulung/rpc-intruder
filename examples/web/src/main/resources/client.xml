<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
               http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!-- dubbo-->
    <dubbo:application name="demo-consumer"/>
    <dubbo:registry address="multicast://224.5.6.7:1234"/>
    <dubbo:reference id="helloService" interface="com.wchukai.service.HelloService"/>

    <!-- rpc intruder-->

    <context:component-scan base-package="com.wchukai"/>
    <bean class="com.wchukai.rpcintruder.client.RpcServiceBeanInit">
        <property name="classNames">
            <list>
                <value>com.wchukai.service.HelloService2</value>
            </list>
        </property>
        <property name="checkEnv" value="false"/>
        <property name="url" value="http://127.0.0.1:8080/rpc-intruder"/>
    </bean>
</beans>

